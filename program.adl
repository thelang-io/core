main {
  a := '1'
  b := 3.14159
  c := 7
  d := "Hello, World!"
  e := a
  f := b
  g := c
  h := d
  mut i := 0
  j := !3
  k := ~4
  l := !~5
  m := -!!1
  n := +~~5
  o := i++
  p := i--
  q := ++i
  r := --i

  print()
  print(d)
  print("a:", a, "b:", b, "c:", c)
  print("e:", e, "f:", f, "g:", g, "h:", h)
  print("i:", i)
  i = 1
  print("i:", i)
  print("j:", j, "k:", k, "l:", l, "m:", m)
  print("n:", n, "o:", o, "p:", p, "q:", q, "r:", r)
}
