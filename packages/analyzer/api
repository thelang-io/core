obj Error {
  message: str
  stack: str
}

obj std_File {
}

obj std {
  fn clearerr (stream: ref std_File) void
  fn fclose (stream: ref std_File) int
  fn feof () void
  fn ferror () void
  fn fflush () void
  fn fgetc () void
  fn fgetpos () void
  fn fgets () void
  fn fgetwc () void
  fn fgetws () void
  fn fopen () void
  fn fprintf () void
  fn fputc () void
  fn fputs () void
  fn fputwc () void
  fn fputws () void
  fn fread () void
  fn freopen () void
  fn fscanf () void
  fn fseek () void
  fn fsetpos () void
  fn ftell () void
  fn fwide () void
  fn fwprintf () void
  fn fwrite () void
  fn fwscanf () void
  fn getc () void
  fn getchar () void
  fn gets () void
  fn getwc () void
  fn getwchar () void
  fn perror () void
  fn printf () void
  fn putc () void
  fn putchar () void
  fn puts () void
  fn putwc () void
  fn putwchar () void
  fn remove () void
  fn rename () void
  fn rewind () void
  fn scanf () void
  fn setbuf () void
  fn setvbuf () void
  fn snprintf () void
  fn sprintf () void
  fn sscanf () void
  fn swprintf () void
  fn swscanf () void
  fn tmpfile () void
  fn tmpnam () void
  fn ungetc () void
  fn ungetwc () void
  fn vfprintf () void
  fn vfscanf () void
  fn vfwprintf () void
  fn vfwscanf () void
  fn vprintf () void
  fn vscanf () void
  fn vsnprintf () void
  fn vsprintf () void
  fn vsscanf () void
  fn vswprintf () void
  fn vswscanf () void
  fn vwprintf () void
  fn vwscanf () void
  fn wprintf () void
  fn wscanf () void
}

obj std_Constants {
  EOF: int
  FOPEN_MAX: int
  FILENAME_MAX: int
  BUFSIZ: int
  SEEK_SET: int
  SEEK_CUR: int
  SEEK_END: int
  TMP_MAX: int
}

const std_constants: std_Constants
