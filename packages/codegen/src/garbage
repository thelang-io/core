export obj Garbage {
  mut items: any[]
  mut len: int

  fn atString (mut self: ref Self, index: int) str {
    return (self.items[index] as any) as str
  }

  fn pushString (mut self: ref Self, item: str) int {
    self.items.push(item)
    self.len++
    return self.len - 1
  }
}
